    "exceljs": "^4.4.0",
    "framer-motion": "^11.3.18",
    "html-react-parser": "^5.1.18",
    "isomorphic-dompurify": "^2.4.0",
    "lodash": "^4.17.21",
    "qs": "^6.13.0",
    "react": "^18.2.0",
    "react-cookie": "^7.1.4",
    "react-dom": "^18.2.0",
    "react-error-boundary": "^4.0.13",
    "react-hook-form": "^7.52.2",
    "react-router-dom": "^7.4.0",
    "react-secure-storage": "^1.3.2",
    "react-virtuoso": "^4.12.0",
