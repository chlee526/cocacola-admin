{
  "name": "cocacola",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "publishConfig": {
    "@chlee526:registry": "https://npm.pkg.github.com"
  },
  "scripts": {
    "dev": "vite",
    "dev:prod": "vite --mode production",
    "build": "tsc && vite build --mode production --outDir @dist/prod",
    "build:dev": "tsc && vite build --mode development --outDir @dist/dev",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "generate": "npm run build:dev && npm run build",
    "test": "vitest run",
    "test:ui": "vitest --ui",
    "no-tag-release": "git checkout devel && node createTag.js notag",
    "no-tag-release:major": "git checkout devel && node createTag.js major notag",
    "no-tag-release:minor": "git checkout devel && node createTag.js minor notag",
    "no-tag-release:patch": "git checkout devel && node createTag.js patch notag",
    "release:major": "git checkout devel && node createTag.js major",
    "release:minor": "git checkout devel && node createTag.js minor",
    "release:patch": "git checkout devel && node createTag.js patch"
  },
  "dependencies": {
